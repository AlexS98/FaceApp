{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SQLLocalConnectionString": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=FaceApp;Integrated Security=True;Connect Timeout=30;Encrypt=False;TrustServerCertificate=False;ApplicationIntent=ReadWrite;MultiSubnetFailover=False",
    "SQLAzureConnectionString": "#{azure_sql}#"

  },
  "FaceApi": {
    "faceEndpoint": "https://facesservice.cognitiveservices.azure.com",
    "faceKey": "#{face_key}#"
  },
  "Auth": {
    "ISSUER": "FaceAppApi",
    "AUDIENCE": "User",
    "KEY": "FaceAppI_secret_KEY",
    "LIFETIME": 24
  }
}
